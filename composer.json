{
    "name": "Cms/DashboardManagerBundle",
    "type": "symfony-bundle",
    "description": "Symfony DashboardManagerBundle",
    "keywords": ["Dashboard management"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "symfony/form": "~2.3",
        "symfony/security-bundle": "~2.3",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.3",
        "swiftmailer/swiftmailer": "~4.3|~5",
        "willdurand/propel-typehintable-behavior": "~1.0",
        "symfony/yaml": "~2.3",
        "symfony/validator": "~2.3"
    },
    "suggest": {
        "willdurand/propel-typehintable-behavior": "Needed when using the propel implementation"
    },
    "autoload": {
        "psr-4": { "Oni\\DashboardManagerBundle\\": "" }
    },
    "support": {
        "docs": "https://symfony.com/doc/master/bundles/FOSUserBundle/index.html"
    }
}